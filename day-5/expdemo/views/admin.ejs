                       <blockquote>
                           <p>&ldquo;<%=db_rows_array[i].message %>&ldquo;</p>
                           <p class="author"><cite>&mdash; <%= db_rows_array[i].name %></cite></p>
                       </blockquote>
                       <a href="/delete/<%=db_rows_array[i].id%>">Delete</a> | <a href="/edit/<%=db_rows_array[i].id%>">Edit</a>
                        
