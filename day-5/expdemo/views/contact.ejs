	</head>
	<body>
		
	<div class="gtco-loader"></div>
	
	<div id="page">

		<%- include('navigator'); -%>
		<div class="gtco-section">
			<div class="gtco-container">
				<div class="row gtco-heading">
					<div class="col-md-7 text-left">
						<h2>Contact</h2>
						<p>Send us a message</p>
                        <p>We would love your feedback </p>
					</div>
					
				</div>
				<div class="row">
					<div class="col-md-6">
						<form method="POST" action="/form-process" >
							<div class="form-group">
								<label for="name">Name</label>
								<input type="text" class="form-control" name="name" required>
							</div>
							<div class="form-group">
								<label for="name">Email</label>
								<input type="text" class="form-control" name="email" required>
							</div>
							<div class="form-group">
								<label for="message">Feedback</label>
								<input type="text" style="height:200px;font-size:14pt;" class="form-control" name="message" required>
							</div>
							<div class="form-group">
							<input type="submit">	
							</div>
						</form>
					</div>
					<% if(db_rows_array.length > 0){ %>
                                
                             <div class="col-md-5 col-md-push-1 gtco-testimonials">
						<h2>Testimonials</h2>

                           <% for(var i=0;i<db_rows_array.length;i++){%>
							<blockquote>
								<p>&ldquo;<%=db_rows_array[i].message %>&ldquo;</p>
								<p class="author"><cite>&mdash; <%= db_rows_array[i].name %></cite></p>
							</blockquote>
							<% } %>

						
						  
					</div>
					
						<%}else { %>
							<%}%>
					
				</div>
			</div>
		</div>
		<!-- END Contact -->

        <%- include('footer'); -%>

	</div>

	<div class="gototop js-top">
		<a href="#" class="js-gotop"><i class="icon-arrow-up"></i></a>
	</div>
	
	<!-- jQuery -->
	<script src="js/jquery.min.js"></script>
